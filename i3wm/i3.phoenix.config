{
  "fontsize": "10",
  "smart_terminal_command": "i3-smart-terminal",
  "smart_terminal_lo_dpi_zoom": 1.15,
  "smart_terminal_hi_dpi_zoom": 1.15,
  "smart_terminal_terminal_command": "mate-terminal",
  "floating_terminal_command": "i3-smart-terminal",
  "floating_terminal_args": "--role=floating_terminal",
  "floating_terminal_window_props": "instance=\"mate-terminal\" window_role=\"floating_terminal\"",
  "pa_sink": "1",
  "settings_manager": "unity-control-center",
  "startup_programs":
    [
    "gnome-settings-daemon",
    "gnome-keyring-daemon --replace --components=pcks11",
    "nitrogen --restore",
    "~/.local/bin/mopidy-as-user.sh"
    ],
  "i3status_config": "~/.dotfiles/i3wm/i3status.phoenix.config"
}
